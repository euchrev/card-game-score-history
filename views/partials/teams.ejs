<table id="teams-leaderboard">
  <thead>
    <tr>
      <th>MEMBERS</th>
      <th>WIN TOTAL</th>
      <th>LOSS TOTAL</th>
      <th>WIN PERCENTAGE</th>
    </tr>
  </thead>
  <% teamLeaderboard.forEach(team => { %>
  <tr>
    <td class="table-name"><%= team.members %></td>
    <td><%= team.wins %></td>
    <td><%= team.losses %></td>
    <td><%= team.winPercentage %></td>
  </tr>
  <% }) %>
</table>
